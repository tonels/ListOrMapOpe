{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "description": "响应状态码"
    },
    "msg": {
      "type": "string",
      "description": "响应描述"
    },
    "success": {
      "type": "boolean",
      "description": "成功与否"
    },
    "result": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "showId": {
            "type": "number",
            "description": "节目ID"
          },
          "assetId": {
            "type": "number",
            "description": "媒资ID"
          },
          "assetName": {
            "type": "string",
            "description": "媒资名称"
          },
          "disposeChannel": {
            "type": "string",
            "description": "输出平台"
          },
          "contentID": {
            "type": "number",
            "description": "内容ID"
          },
          "disposeTime": {
            "type": "string",
            "description": "输出时间"
          }
        },
        "required": []
      },
      "description": "响应结果"
    }
  }
}